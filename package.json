  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "formik": "^2.1.4",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1",
    "typescript": "~3.7.2",
    "validate.js": "^0.13.1"
  },
  "scripts": {
    "typecheck": "tsc",
    "start": "react-scripts start",
    "prettier": "prettier --write \"src/**/*.{js,tsx,ts,css}\"",
    "prettier:check": "prettier --check \"./src/**/*.{tsx,ts,js,css}\"",
    "lint": "eslint \"./src/**/*.{tsx,ts,js}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{tsx,ts,js}\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "eslintIgnore": [
      "src/**/__generated__/**"
