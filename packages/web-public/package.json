{
  "name": "neighborgoods",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@bugsnag/js": "^6.5.0",
    "@bugsnag/plugin-react": "^6.5.0",
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "bugsnag": "^2.4.3",
    "classnames": "^2.2.6",
    "formik": "^2.1.4",
    "lodash": "^4.17.15",
    "material-ui-dropzone": "^2.4.7",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-datepicker": "^2.12.1",
    "react-day-picker": "^7.4.0",
    "react-dom": "^16.12.0",
    "react-relay-network-modern": "4.5.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1",
    "react-swipeable-views": "^0.13.9",
    "react-use": "^13.27.0",
    "relay-hooks": "^3.0.0",
    "socket.io-client": "^2.3.0",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "~3.7.2",
    "validate.js": "^0.13.1",
    "yup": "^0.28.1"
  },
  "scripts": {
    "typecheck": "tsc",
    "start": "react-scripts start",
    "prettier": "prettier --write \"src/**/*.{js,tsx,ts,css}\"",
    "prettier:check": "prettier --check \"./src/**/*.{tsx,ts,js,css}\"",
    "lint": "eslint \"./src/**/*.{tsx,ts,js}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{tsx,ts,js}\"",
    "build": "react-scripts build",
    "relay": "relay-compiler --src ./src --schema src/schemas/schema.graphql --language typescript",
    "update-schema": "graphql get-schema --project neighborgoods",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "eslintIgnore": [
      "src/**/__generated__/**"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "bracketSpacing": false
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/lodash": "^4.14.149",
    "@types/material-ui": "^0.21.7",
    "@types/moment": "^2.13.0",
    "@types/react-datepicker": "^2.11.0",
    "@types/react-day-picker": "^5.3.0",
    "@types/react-relay": "^7.0.3",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/relay-runtime": "^8.0.5",
    "@types/socket.io-client": "^1.4.32",
    "@types/validate.js": "^0.11.0",
    "@types/yup": "^0.26.30",
    "babel-plugin-relay": "^7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "graphql": "^14.3.1",
    "prettier": "^1.19.1",
    "react-relay": "^9.0.0",
    "relay-compiler": "^7.1.0",
    "relay-compiler-language-typescript": "^10.1.4"
  }
}
